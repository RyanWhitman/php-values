{
    "name": "ryanwhitman/php-values",
    "description": "",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "RyanWhitman\\PhpValues\\": "src/"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0 || ^8.1"
    },
    "require-dev": {
        "filp/whoops": "^2.14",
        "oyova/php-cs-fixer": "^1.2",
        "pestphp/pest": "^1.22",
        "symfony/var-dumper": "^5.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
